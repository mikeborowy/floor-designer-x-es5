<div id="finder-search-panel">
    <div id="finder-search-box">
        <group class="finder-search-group">
            <div class="mdl-list__item mdl-list__item--two-line">
                <span class="mdl-list__item-primary-content">
                    <i class="material-icons mdl-list__item-avatar">business</i>
                    <span>Office 1</span>
                    <span class="mdl-list__item-sub-title">Malborska St. 51, Warsaw 03-234</span>
                </span>

            </div>
            <div class="mdl-list__item-primary-content">
                <form id="search-room-form">
                    <group class="search-room-form-group">
                        <div class="mdl-textfield mdl-js-textfield mdl-textfield--floating-label">
                            <input class="mdl-textfield__input floor-textfield" type="text" id="first-name">
                            <label class="mdl-textfield__label floor-textfield" for="first-name">First Name</label>
                        </div>
                    </group>
                    <group class="search-room-form-group">
                        <div class="mdl-textfield mdl-js-textfield mdl-textfield--floating-label">
                            <input class="mdl-textfield__input floor-textfield" type="text" id="last-name">
                            <label class="mdl-textfield__label floor-textfield" for="last-name">Last Name</label>
                        </div>
                    </group>
                    <group class="search-room-form-group">
                        <button id="find-room-btn"
                                type="submit"
                                class="mdl-button mdl-js-button mdl-button--raised mdl-js-ripple-effect mdl-button--accent">
                            <i class="material-icons">search</i>
                            Find Room
                        </button>
                    </group>
                </form>
            </div>
            <span class="mdl-list__item-secondary-content">
            </span>
        </group>
    </div>
</div>