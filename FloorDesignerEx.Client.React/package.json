{
  "name": "floor-designer-ex-app",
  "version": "0.1.0",
  "description": "Example React with WebPack in ASP.NET MVC5",
  "author": "Mike Borowy",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev-build": "webpack -d --config webpack.config.js --colors",
    "dev-watch": "webpack -d --config webpack.config.js --watch --hot --colors --port 3000",
    "prod-build": "SET NODE_ENV=production&&webpack -p --config webpack.config.js --colors"
  },
  "devServer": {
    "inline": true,
    "contentBase": "./src",
    "port": 3000
  },
  "dependencies": {
    "axios": "^0.17.1",
    "babel-polyfill": "6.8.0",
    "babel-preset-stage-1": "^6.24.1",
    "gsap": "^1.20.2",
    "jquery": "^3.2.1",
    "json-loader": "^0.5.7",
    "lodash": "^4.17.4",
    "material-design-lite": "^1.3.0",
    "react": "^15.6.1",
    "react-addons-css-transition-group": "^15.6.0",
    "react-addons-transition-group": "^15.6.2",
    "react-addons-update": "^15.6.0",
    "react-dnd": "^2.5.1",
    "react-dnd-html5-backend": "^2.5.1",
    "react-dom": "^15.6.1",
    "react-router": "^4.2.0",
    "react-transition-group": "^2.2.1"
  },
  "devDependencies": {
    "babel-core": "6.8.0",
    "babel-loader": "6.2.4",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "react-hot-loader": "^1.3.1",
    "extract-text-webpack-plugin": "1.0.1",
    "file": "^0.2.2",
    "file-loader": "^0.10.0",
    "json-loader": "^0.5.4",
    "html-webpack-plugin": "2.16.0",
    "optimize-css-assets-webpack-plugin": "^1.3.0",
    "css-loader": "^0.26.1",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1",
    "url": "^0.11.0",
    "url-loader": "^0.5.7",
    "webpack": "1.13.0",
    "webpack-dev-server": "1.13.0"
  },
  "-vs-binding": {
    "ProjectOpened": [
      "dev-watch"
    ]
  }
}
